~ Test 1: Lists Only
say "--- Test 1: Verifying List Indexing ---"

let my_list = [10, 20, 30]
say "  -> Initial list: " + str(my_list)

my_list[1] = 25 ~ Modify
say "  -> Modified list: " + str(my_list)

let item = my_list[1] ~ Access
say "  -> Accessed item: " + str(item)

say "\n--- List Test Finished ---"